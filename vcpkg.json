{
    "name": "qt-simple-template",
    "version": "1.0.0",
    "description": "Example based on C++ + Qt6 + Vcpkg + GithubCI   ",
    "homepage": "https://github.com/AstroAir/qt-simple-template",
    "license": "MIT",
    "dependencies": [
        "qtbase",
        "qtspeech",
        "qtsvg",
        "fmt",
        "spdlog",
        {
            "name": "qttools",
            "features": [
                "linguist"
            ]
        },
        {
            "name": "poppler",
            "features": [
                "qt"
            ]
        },
        {
            "name": "vcpkg-cmake",
            "host": true
        },
        {
            "name": "vcpkg-cmake-config",
            "host": true
        }
    ],
    "overrides": [
        {
            "name": "fmt",
            "version": "11.2.0"
        },
        {
            "name": "spdlog",
            "version": "1.15.3"
        }
    ]
}
